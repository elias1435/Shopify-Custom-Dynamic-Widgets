}, { "type": "range", "id": "btn_radius", "label": "Button radius (px)", "min":
0, "max": 40, "step": 1, "default": 8 }, { "type": "range", "id": "btn_pad_y",
"label": "Button padding Y (px)", "min": 8, "max": 32, "step": 1, "default": 16
